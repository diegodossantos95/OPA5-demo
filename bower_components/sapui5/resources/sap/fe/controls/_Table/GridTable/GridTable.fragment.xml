<core:FragmentDefinition
		xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1"
		xmlns="sap.m"
		xmlns:core="sap.ui.core"
		xmlns:table="sap.ui.table"
		height="100%">

	<table:Table
			rows="{parts : [{path: 'entitySet>'}, {path: 'this>tableBindingPath'}, {path: 'this>filterBarId'}, {path: 'lineItem>'}], formatter: 'sap.fe.controls._Table.TableAnnotationHelper.createAggregationBinding'}"
			rowSelectionChange="handleSelectionChange"
			enableBusyIndicator="true"
			visibleRowCount="15"
			threshold="20"
			enableColumnFreeze="true">
		<table:extension>
			<core:Fragment fragmentName="sap.fe.controls._Table.TableToolbar" type="XML"/>
		</table:extension>
		<table:columns>
			<template:repeat list="{lineItem>}" var="dataField">
				<template:if
						test="{= !(${dataField>$Type} === 'com.sap.vocabularies.UI.v1.DataFieldForAction' &amp;&amp; ${dataField>Inline} !== true) &amp;&amp; !(${dataField>Value/$Path@com.sap.vocabularies.UI.v1.Hidden} === true) &amp;&amp; !(${dataField>Value@com.sap.vocabularies.UI.v1.Hidden} === true) }">
					<core:Fragment fragmentName="sap.fe.controls._Table.GridTable.GridTableColumn" type="XML"/>
				</template:if>
			</template:repeat>
		</table:columns>
	</table:Table>
</core:FragmentDefinition>
